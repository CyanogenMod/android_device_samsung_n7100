#!/system/bin/sh
#
# Copy camera firmware to /data/cfw
#

busybox cp /system/vendor/firmware/SlimISP_GD.bin /data/cfw/SlimISP_GK.bin
busybox cp /system/vendor/firmware/SlimISP_ZD.bin /data/cfw/SlimISP_ZK.bin
busybox chown system /data/cfw/*
busybox chgrp media /data/cfw/*
busybox chmod 0775 /data/cfw/*
